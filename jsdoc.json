{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [
            "./js"
        ]
    },
    "plugins": [
        "better-docs/component",
        "better-docs/category",
        "plugins/markdown"
    ],
    "opts": {
        "encoding": "utf8",
        "destination": "docs/",
        "readme": "readme.md",
        "recurse": true,
        "verbose": true,
        "template": "node_modules/better-docs"
    },
    "templates": {
        "better-docs": {
            "name": "Documentação UseallTestFramework",
            "logo": "images/logo-useall.png",
            "navigation": [
                {
                    "label": "Github",
                    "href": "https://github.com/cassiourugit/UseallTestFramework"
                },
                {
                    "label": "Npm Package",
                    "href": "https://www.npmjs.com/package/usealltestframework"
                }
            ]
        }
    }
}